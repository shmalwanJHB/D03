void     ft_ultimate_div_mod(int a, int *div, int b, int *mod)
{	
	int p = 0;
	if (p! = '\0')
	{
		*mod = (a % b);
		*div = (a / b);
	}
}

int main(void)
{	
	int x;  int a;
	int y;  int b;
	
  x = 6;	
  y = 17;
	
	ft_ultimate_div_mod(x, &a, y, &b);
	printf(("x = %i) && (y = %i\n", x, y))
		printf("div = %i && mod = %i\n", a, b);
}
